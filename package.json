{
  "name": "guitar-journey",
  "version": "0.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build -c production",
    "test": "npx jest --coverage",
    "e2e": "ng e2e",
    "deploy": "cd dist && zip -r -X guitar-practice.zip ./*",
    "lint": "tsc --noEmit && eslint . --ext js,ts,json --quiet --fix",
    "compodoc": "npx compodoc -p tsconfig.doc.json -d docs",
    "web": "ng serve --port 4200",
    "emu": "firebase emulators:start --only auth,firestore,storage",
    "emu:all": "firebase emulators:start",
    "seed": "cross-env FIRESTORE_EMULATOR_HOST=localhost:8080 FIREBASE_AUTH_EMULATOR_HOST=localhost:9099 FIREBASE_STORAGE_EMULATOR_HOST=localhost:9199 node dist-tools/seed.js",
    "dev": "concurrently -s first -n EMU,WEB -c blue,green \"npm run emu\" \"wait-on tcp:8080 tcp:9099 tcp:9199 && npm run seed && npm run web\"",
    "preview": "ng build && firebase emulators:start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.6",
    "@angular/cdk": "^20.1.5",
    "@angular/common": "^20.1.6",
    "@angular/compiler": "^20.1.6",
    "@angular/core": "^20.1.6",
    "@angular/fire": "^20.0.1",
    "@angular/forms": "^20.1.6",
    "@angular/material": "^20.1.5",
    "@angular/platform-browser": "^20.1.6",
    "@angular/platform-browser-dynamic": "^20.1.6",
    "@angular/router": "^20.1.6",
    "@angular/service-worker": "^20.1.6",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@firebasegen/default-connector": "file:dataconnect-generated/js/default-connector",
    "@primeuix/themes": "^1.2.3",
    "aws-sdk": "^2.1692.0",
    "dayjs": "^1.11.13",
    "eev": "^0.1.5",
    "express": "^5.1.0",
    "firebase": "^12.1.0",
    "firebase-tools": "^14.12.1",
    "gh": "^2.8.9",
    "note-parser": "^2.0.1",
    "npm": "^11.5.2",
    "path": "^0.12.7",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.0.1",
    "rxjs": "^7.8.2",
    "svguitar": "^2.4.1",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0",
    "yarn": "^1.22.22",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^20.0.0",
    "@angular-devkit/build-angular": "^20.1.5",
    "@angular-eslint/schematics": "^20.1.1",
    "@angular/cli": "^20.1.5",
    "@angular/compiler-cli": "^20.1.6",
    "@angular/core": "^20.1.6",
    "@angular/forms": "^20.1.6",
    "@angular/language-service": "^20.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "firebase-admin": "^13.4.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "~10.9.2",
    "typescript": "^5.8.2",
    "wait-on": "^8.0.4"
  },
  "jest-junit": {
    "outputDirectory": "./reports",
    "outputName": "junit.xml"
  }
}
